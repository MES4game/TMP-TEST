{
    "name": "site-bde-front",
    "version": "1.0.0",
    "description": "Front-end of BDE website",
    "private": true,
    "type": "module",
    "scripts": {
        "lint": "node scripts/lint.mjs",
        "dev": "node scripts/dev.mjs",
        "build": "node scripts/build.mjs",
        "lint:run": "eslint 'src/**/*.[jt]s?(x)' --max-warnings=0 --cache",
        "nibble:run": "eslint-nibble 'src/**/*.[jt]s?(x)' --cache",
        "dev:run": "NODE_ENV='development' webpack serve --config webpack.config.mjs",
        "build:run": "NODE_ENV='production' webpack build --config webpack.config.mjs"
    },
    "keywords": [
        "typescript",
        "eslint",
        "webpack",
        "react",
        "react-router",
        "tailwindcss",
        "material-ui",
        "fontawesome"
    ],
    "author": "CIA - Polytech Paris-Saclay",
    "license": "MIT",
    "dependencies": {
        "@emotion/react": "11.14.0",
        "@emotion/styled": "11.14.1",
        "@fontsource/roboto": "5.2.8",
        "@fortawesome/free-brands-svg-icons": "7.1.0",
        "@fortawesome/free-regular-svg-icons": "7.1.0",
        "@fortawesome/free-solid-svg-icons": "7.1.0",
        "@fortawesome/react-fontawesome": "3.1.0",
        "@mui/icons-material": "7.3.4",
        "@mui/material": "7.3.4",
        "react": "19.2.0",
        "react-dom": "19.2.0",
        "react-router-dom": "7.9.3"
    },
    "devDependencies": {
        "@eslint/js": "9.34.0",
        "@pmmmwh/react-refresh-webpack-plugin": "0.6.1",
        "@stylistic/eslint-plugin": "5.4.0",
        "@tailwindcss/postcss": "4.1.14",
        "@types/node": "^24.6.2",
        "@types/react": "19.2.0",
        "@types/react-dom": "19.2.0",
        "@typescript-eslint/eslint-plugin": "8.45.0",
        "@typescript-eslint/parser": "8.45.0",
        "css-loader": "7.1.2",
        "eslint": "9.37.0",
        "eslint-nibble": "9.0.0",
        "eslint-plugin-react": "7.37.5",
        "html-webpack-plugin": "5.6.4",
        "mini-css-extract-plugin": "2.9.4",
        "node": "^24.9.0",
        "postcss": "8.5.6",
        "postcss-loader": "8.2.0",
        "react-refresh": "0.18.0",
        "style-loader": "4.0.0",
        "tailwindcss": "4.1.14",
        "ts-loader": "9.5.4",
        "typescript": "5.9.3",
        "typescript-eslint": "8.45.0",
        "webpack": "5.102.0",
        "webpack-cli": "6.0.1",
        "webpack-dev-server": "5.2.2"
    }
